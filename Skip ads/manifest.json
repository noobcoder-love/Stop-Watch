{
    "manifest_version": 3,
    "name": "YouTube Ad Skipper",
    "version": "1.0",
    "description": "Automatically skips YouTube ads.",
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
    
  }
  