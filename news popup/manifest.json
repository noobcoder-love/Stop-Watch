{
    "manifest_version": 3,
    "name": "Daily News",
    "version": "1.0",
    "description": "Fetches daily news",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  