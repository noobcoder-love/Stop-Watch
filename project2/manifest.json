{
    "manifest_version": 3,
    "name": "Stopwatch Extension",
    "version": "1.0",
    "description": "A simple stopwatch with Start and Stop buttons",
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage"]
  }
  