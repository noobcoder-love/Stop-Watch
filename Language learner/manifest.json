{
    "manifest_version": 3,
    "name": "TranslateToHindi",
    "version": "1.0",
    "description": "Translates selected text into Hindi.",
    "permissions": [
      "contextMenus",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icons-16.png.jpg",
      "32": "icons/icons-32.png.jpg",
      "48": "icons/icons-48.png.jpg",
      "128": "icons/icons-128.png.jpg"
    }
    },
    "icons": {
    "16": "icons/icons-16.png.jpg",
    "32": "icons/icons-32.png.jpg",
    "48": "icons/icons-48.png.jpg",
    "128": "icons/icons-128.png.jpg"
  }
    
  }
  