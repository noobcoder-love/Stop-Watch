{
    "manifest_version": 3,
    "name": "Customization and Personalization Extension",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "commands",
      "identity"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "commands": {
      "custom_shortcut": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y"
        },
        "description": "Custom shortcut"
      }
    }
  }
  